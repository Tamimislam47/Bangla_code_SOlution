{
    "compilerOptions": {
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "allowJs": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "module": "ESNext",
        "target": "ESNext",
        "baseUrl": ".",
        "paths": {
            "@/components/*": [
                "./src/components/*"
            ],
            "@/lib/*": [
                "./src/lib/*"
            ]
        }
    }
}